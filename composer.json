{
    "name": "kontoulis/rabbit-manager",
    "description": "Standalone RabbitMQ Command line Manager",
    "license": "GNU2",
    "keywords": ["rabbitmq"],
    "authors": [
        {
            "name": "Kontoulis Dimitris",
            "email": "kontoulisdhm@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/KontoulisDimitrios/rabbitmq-manager"
        }
    ],
    "require": {
        "videlalvaro/php-amqplib": "2.5.*",
        "monolog/monolog": "1.*",
