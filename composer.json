{
    "name": "kontoulis/rabbit-manager",
    "description": "Standalone RabbitMQ Command line Manager",
    "license": "MIT",
    "keywords": ["rabbitmq"],
    "authors": [
        {
            "name": "Kontoulis Dimitris",
            "email": "kontoulisdhm@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kontoulis/rabbitmq-manager"
        }
    ],
    "require": {
        "php-amqplib/php-amqplib": "2.6.*",
        "monolog/monolog": "1.*",
        "symfony/console": "^2.7",
        "swiftmailer/swiftmailer": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "RabbitManager\\": "src"
        }
    },
    "extra": {
    "branch-alias": {
        "dev-master": "1.0.1-dev"
    }
},
    "minimum-stability": "dev"

}
